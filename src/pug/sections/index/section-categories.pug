.categories
  .categories__body
    .categories__content
      each category in categories
        a(href="#").card.categories__card
          .card__body
            .card__header
              .card__title= category.title
              .card__button
                +iconfont('arrow-up', 'icon')
            .card__content
              +picture('/img/categories/' + category.image, 'image')
