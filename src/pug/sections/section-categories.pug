.categories
  .categories__body
    .categories__content
      each category in categories
        .card.categories__card
          .card__body
            .card__header
              .card__title= category.title
              .card__button
                +iconfont('arrow-up', '_icon')
            .card__content
              +picture(webRoot + 'img/categories/' + category.image, 'image')
